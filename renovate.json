{
  "extends": [
    "config:base",
    ":gitSignOff",
    ":disableDependencyDashboard"
  ],
  "ignorePaths": [
    ".github/**"
  ],
  "postUpgradeTasks": {
    "commands": [
      "make gen-golden-all",
    ],
    "fileFilters": [
      "component/tests/golden/**",
      "package/tests/golden/**"
    ],
    "executionMode": "update"
  },
  "suppressNotifications": [
    "artifactErrors"
  ],
  "labels": [
    "dependency"
  ]
}
